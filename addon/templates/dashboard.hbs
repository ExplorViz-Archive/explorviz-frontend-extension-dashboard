<link href="/css/sb-admin-2.min.css" rel="stylesheet">


{{modal}}

<div id="content-wrapper" class="d-flex flex-column" style="height:100%; overflow-y:scroll;">

  <!-- Main Content -->
  <div id="content">


    <!-- Begin Page Content -->
    <div class="container-fluid">

      <!-- Page Heading -->
      <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Dashboard</h1>
        <a href="dashboardsettings" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i class="fas fa-download fa-sm text-white-50"></i> Dashboard Settings</a>
      </div>

      <!-- Content Row -->
      <div class="row">


        <!-- Dropdown Card  -->



        {{#each this.widgets as |widgetList|}}

          {{#if (isnotequal widgetList.widgetName "empty")}}
            {{component widgetList.widgetName id=widgetList.instanceID class="col-xl-4 col-lg-5"}}
          {{else}}
            {{alert}}
          {{/if}}



        {{/each}}

      </div>


    </div>


  </div>
  <!-- End of Main Content -->



</div>
